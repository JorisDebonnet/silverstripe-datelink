{
	"name": "tractorcow/silverstripe-datelink",
	"description": "Allows you to add year/month prefixes to the paths inside time-sensitive sections of your website",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "url", "date"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Damian Mooyman",
			"email": "damian.mooyman@gmail.com"
		}
	],
	"support": {
		"issues": "http://github.com/tractorcow/silverstripe-datelink/issues"
	},
	"require": {	
		"silverstripe/framework": ">4"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.x-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"TractorCow\\DateLink\\": "src/"
		}
	}
}
